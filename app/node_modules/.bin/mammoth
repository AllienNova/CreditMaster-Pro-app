#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/creditmaster-pro/node_modules/.pnpm/mammoth@1.10.0/node_modules/mammoth/bin/node_modules:/home/ubuntu/creditmaster-pro/node_modules/.pnpm/mammoth@1.10.0/node_modules/mammoth/node_modules:/home/ubuntu/creditmaster-pro/node_modules/.pnpm/mammoth@1.10.0/node_modules:/home/ubuntu/creditmaster-pro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/creditmaster-pro/node_modules/.pnpm/mammoth@1.10.0/node_modules/mammoth/bin/node_modules:/home/ubuntu/creditmaster-pro/node_modules/.pnpm/mammoth@1.10.0/node_modules/mammoth/node_modules:/home/ubuntu/creditmaster-pro/node_modules/.pnpm/mammoth@1.10.0/node_modules:/home/ubuntu/creditmaster-pro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/mammoth@1.10.0/node_modules/mammoth/bin/mammoth" "$@"
else
  exec node  "$basedir/../.pnpm/mammoth@1.10.0/node_modules/mammoth/bin/mammoth" "$@"
fi
